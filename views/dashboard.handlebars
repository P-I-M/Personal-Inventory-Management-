         <div class="iframediv"> 
  <iframe id="frame" class="responsive-iframe" src="/dashboard/products" frameborder="0"></iframe>
</div>

  </iframe>
  {{#if products.length}}
  <iframe class="responsive-iframe" src="/dashboard/calendar" frameborder ="0"></iframe>
    <section>
      <h2>Your products</h2>
      {{#each products as |product|}}
          {{> calendar product}}
          <p>{{product.product_name}} / {{product.stock}}</p>
      {{/each}}
    </section>
  </iframe>
  {{/if}}


</div>

