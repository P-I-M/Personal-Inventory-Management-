<button type="button" onClick="document.getElementById('frame').src='/dashboard/products'">My Inventory</button>
<button type="button" onClick="document.getElementById('frame').src='/dashboard/suggestions'">Daily Suggestions</button>
<button type="button" onClick="document.getElementById('frame').src='/dashboard/new'">Add Item</button>
<button type="button" onClick="document.getElementById('frame').src='/dashboard/delete'">Remove Item</button>

  </div>

  

  <div class="scheduler">
    <!-- Monday  -->
          <div class="input-group mborder">
            <div class="input-group-prepend row">
              <span class="input-group-text day">Monday:</span>
            </div>
            <textarea value="" id="entryMonday" type="text" cols="100" rows="2" class="form-control description textarea"></textarea>
            <div class="input-group-append">
              <button id="buttonMonday" class="btn btn-outline-secondary saveBtn" type="button"><i
                  class="far fa-save"></i></button>
            </div>
          </div>
      
          <!-- Tuesday -->
          <div class="input-group mborder .col-lg-">
            <div class="input-group-prepend row">
